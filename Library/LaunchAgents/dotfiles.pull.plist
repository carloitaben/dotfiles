<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>dotfiles.pull.plist</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/sh</string>
    <string>-c</string>
    <string>/usr/bin/git -C "$HOME/.dotfiles" pull --ff-only</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>StandardOutPath</key>
  <string>$HOME/Library/Logs/dotfiles.pull.log</string>
  <key>StandardErrorPath</key>
  <string>$HOME/Library/Logs/dotfiles.pull.err.log</string>
</dict>
</plist>
